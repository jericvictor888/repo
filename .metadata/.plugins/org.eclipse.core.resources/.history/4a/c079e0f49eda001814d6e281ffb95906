package com.random.controllers;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;
import org.springframework.http.ResponseEntity;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RestController;

import com.random.enums.Season;

@RestController(value="/test")
public class Random {
	private static final Logger log = LoggerFactory.getLogger(Random.class);
	
	@GetMapping(value = "/test/enums")
	public ResponseEntity<?> enums(){
		
		Season season = Season.WINTER;  
		log.info("season.WINTER : " + season.WINTER.getValue());
				
		return null;
		
	}
}
